    <questionvariables>
      <text><![CDATA[ordergreat(x,y,z,k)
M1:matrix([2+rand(3),1,k],[1,0,1+rand(2)],[0,1,1]);
M2:transpose(matrix([1+rand(3),1+rand(5),-2-rand(3)]));
M3:M1.transpose([x,y,z]);
S:zip_with("=",first(args(transpose(M3))),first(args(transpose(M2))));
S:transpose(S);
S1:determinant(M1);
S2:solve(S1=0);
ta1:rhs(first(S2));
ws1:apply(matrix,zip_with(append,args(M1),args(M2)))
ws2:ws1[1][1];
ws3:matrix(ws1[1],ws2*ws1[2]+ws1[3],ws1[3]);]]></text>
